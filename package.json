{
  "name": "reinvest-api",
  "version": "1.0.0",
  "description": "ReInvest API",
  "main": "Reinvest/index.ts",
  "scripts": {
    "lint": "eslint ./Reinvest ./serverless.ts devops --ext .ts --config ./eslintrc.json",
    "lint:fix": "yarn lint --fix",
    "test:characterization": "ts-mocha -r tsconfig-paths/register -p tsconfig.json --timeout 10000 test/characterization/**/*.test.ts",
    "bdd": "ts-mocha -r tsconfig-paths/register -p tsconfig.json --timeout 10000 'Reinvest/**/*.feature.ts'",
    "tdd": "ts-mocha -r tsconfig-paths/register -p tsconfig.json --timeout 10000 'Reinvest/**/*.test.ts'",
    "deploy:staging": "NODE_ENV=staging serverless deploy -s staging",
    "deploy:staging:api": "NODE_ENV=staging serverless deploy function --function api -s staging ",
    "destroy:staging": "NODE_ENV=staging serverless remove -s staging",
    "deploy:production": "NODE_ENV=production serverless deploy -s production",
    "destroy:production": "NODE_ENV=production serverless remove -s production",
    "logs:api": "sls logs -s staging -f api -t",
    "local": "serverless offline start -s staging --noAuth --reloadHandler"
  },
  "author": "Lukasz Duraj <https://www.linkedin.com/in/%C5%82ukasz-duraj-a4872317b/>",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^4.3.0",
    "@as-integrations/aws-lambda": "^1.1.0",
    "@aws-sdk/client-s3": "^3.231.0",
    "@aws-sdk/s3-request-presigner": "^3.231.0",
    "@graphql-tools/merge": "^8.3.14",
    "@graphql-tools/mock": "^8.7.14",
    "@graphql-tools/schema": "^9.0.12",
    "axios": "^0.27.2",
    "dotenv": "^16.0.2",
    "express": "^4.18.2",
    "graphql": "^16.6.0",
    "moment": "^2.29.4",
    "postgres": "^3.3.2",
    "serverless-http": "^3.1.0",
    "typed-inject": "^3.0.1"
  },
  "devDependencies": {
    "@serverless/typescript": "serverless/typescript",
    "@types/chai": "^4.3.3",
    "@types/express": "^4.17.14",
    "@types/mocha": "^9.1.1",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "chai": "^4.3.6",
    "esbuild": "^0.16.1",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "serverless": "^3.25.1",
    "serverless-bundle": "^5.5.0",
    "serverless-domain-manager": "^6.2.1",
    "serverless-esbuild": "^1.33.2",
    "serverless-offline": "^11.6.0",
    "serverless-offline-sqs": "^7.3.1",
    "serverless-offline-watcher": "^1.0.0",
    "serverless-stack-termination-protection": "^2.0.2",
    "ts-loader": "^9.4.2",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "ts-sinon": "^2.0.2",
    "typescript": "^4.9.3"
  }
}
